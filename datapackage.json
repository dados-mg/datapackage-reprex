    {
      "name": "data-types",
      "path": "data/file.csv",
      "profile": "tabular-data-resource",
      "schema": "tableschema.json",
      "dialect": "dialect.yaml",
      "title": "Tipos de Dados",
      "format": "csv",
      "encoding": "UTF-8",
      "description": "Esse recurso visa utilizar vários tipos diferentes de dados que aparecem na [Frictionless Specifications\n](https://specs.frictionlessdata.io/).\n\n## Tipos de dados\n\n- string\n- number\n- integer\n- boolean\n- date\n- datetime\n- year\n- yearmonth"
    },
    {
      "name": "path-as-url",
      "title": "Recurso com URL externa",
      "path": "https://raw.githubusercontent.com/dados-mg/letters-datapackage/master/data/letters-consonants.csv",
      "description": "_NICE_ HELLO you `code`",
      "schema": 
      {
              "fields": [
                {
                  "name": "id",
                  "type": "integer",
                  "format": "default",
                  "title": "Incremental counter",
                  "description": "An integer counter for the rows."
                },
                {
                  "name": "letter",
                  "type": "string",
                  "format": "default",
                  "title": "Alfhabet letter",
                  "description": "A lower case alfabet letter",
                  "constraints": {
                    "enum": ["a", "e", "i", "o", "u"]
                  }
                },
                {
                  "name": "vowel",
                  "type": "boolean",
                  "format": "default",
                  "title": "Grammer of letter",
                  "description": "Is the letter a vowel?"
                }
              ]
      }
    }
  ],
